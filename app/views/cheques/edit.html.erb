<div class="row">
  <h2>Editar Factura</h2>
</div>

<div class="row">
  <div class="col-md-8">
    <div class="row">
      <%= render 'form' %>
    </div>

    <div class="row">
      <a type="button" class="btn btn-danger col-md-3" href="<%= obra_caja_cheque_path(@cheque.chequera.obra, @cheque.chequera, @cheque) %>" data-confirm="Estas seguro?" data-method="delete" rel="nofollow">
        <span class="glyphicon glyphicon-remove-circle"></span>
        Borrar Cheque
      </a>
      <a type="button" class="btn btn-warning col-md-3 col-md-offset-1" href="<%= obra_caja_cheque_path(@cheque.chequera.obra, @cheque.chequera, @cheque) %>">
        <span class="glyphicon glyphicon-eye-open"></span>
        Volver a Cheque
      </a>
      <a type="button" class="btn btn-success col-md-3 col-md-offset-2" id="btnGuardar">
        <span class="glyphicon glyphicon-ok"></span>
        Guardar Cambios
      </a>
    </div>
  </div>
</div>
