<h2><%= titulo_vista_cheque %></h2>

<div class="col-md-8">
  <div class="row">
    <%= render 'form' %>
  </div>

  <div class="row">
    <a type="button" class="btn btn-success col-md-3" href="<%= obra_caja_path(@obra, @cheque.chequera) %>">
      <span class="glyphicon glyphicon-list-alt"></span>
      Ver chequera
    </a>
    <a type="button" class="btn btn-warning col-md-3 col-md-offset-1" href="<%= edit_obra_caja_cheque_path(@obra, @cheque.chequera, @cheque) %>">
      <span class="glyphicon glyphicon-edit"></span>
      Editar Cheque
    </a>
  </div>
</div>

<div class="col-md-4">
  <h4>Recibos</h4>
  <%= render 'recibos/list' %>
</div>
