<li class="dropdown">
  <a href="#" class="dropdown-toggle" data-toggle="dropdown">
  <% if @obra %>
  <%= @obra.nombre %>
  <% else %>
  Todas las Obras
  <% end %>
  <b class="caret"></b></a>
  <ul class="dropdown-menu">
    <li>
      <%= link_to 'Todas las Obras', url_for(params.merge(obra_id: nil)) %>
