<table class="table table-hover table-condensed">
  <thead>
    <tr>
      <th></th>
      <th>Obra</th>
      <th>Tipo de Caja</th>
      <th>Saldo</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% cajas.each do |caja| %>
      <tr class="ir-a" data-uri="<%= caja_path(caja.id)%>">
        <td><%= link_to '', [caja], class:"glyphicon glyphicon-eye-open" %></td>
        <td><%= caja.obra.nombre %></td>
        <td><%= caja.tipo %></td>
        <td><%= saldo_movimientos (caja.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

