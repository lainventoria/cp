<h2>Nueva Retencion</h2>

<div class="col-md-8">
  <div class="row">
    <%= render 'form' %>
  </div>

  <div class="row">
    <a type="button" class="btn btn-warning col-md-3 col-md-offset-4" href="<%= con_obra? factura_path (@factura) %>">
      <span class="glyphicon glyphicon-eye-open"></span>
      Volver a Factura
    </a>
    <a type="button" class="btn btn-success col-md-3 col-md-offset-2" id="btnGuardar">
      <span class="glyphicon glyphicon-ok"></span>
      Guardar Cambios
    </a>
  </div>

  <div class="row">
    <br>
  </div>

  <div class="row">
    <a type="button" class="btn btn-warning col-md-3 col-md-offset-4" href="<%= con_obra? retenciones_path %>">
      <span class="glyphicon glyphicon-list"></span>
      Listar Retenciones
    </a>
  </div>
</div>
